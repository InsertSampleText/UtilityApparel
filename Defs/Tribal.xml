                    </female>
                    <thin>
                        <offset>(-0.2,0)</offset>
                    </thin>
                    <hulk>
                        <offset>(-0.2,0)</offset>
                    </hulk>
                    <fat>

                    </fat>
                </east>
                <west>
                    <male>
                        <offset>(0.2,0)</offset>
                    </male>
                    <female>
                        <offset>(0.2,0)</offset>
                    </female>
                    <thin>
                        <offset>(0.2,0)</offset>
                    </thin>
                    <hulk>
                        <offset>(0.2,0)</offset>
                    </hulk>
                    <fat>

